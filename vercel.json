{
    "builds": [{
        "src": "in_office_delivery/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.9", "installCommand": "pip install --use-deprecated=legacy-resolver -r requirements.txt"}
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "in_office_delivery/wsgi.py"
        }
    ]
}

