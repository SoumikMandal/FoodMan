{
    "builds": [{
        "src": "in_office_delivery/wsgi.py",
        "use": "@ardnt/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "in_office_delivery/wsgi.py"
        }
    ]
}